import random

chars = "abcdefghijklmnopqrstuvwyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890@#$&!"

while 1:
    password_len = int(input("How long you want this password cuh? : "))
    password_count = int(input("How many password you want cuz? : "))
    for x in range(0,password_count) :
        password =""
        for x in range(0,password_len) :
            password_char = random.choice(chars)
            password      = password + password_char
        print("Here is your stupid password:", password)
